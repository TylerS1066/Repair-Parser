    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Testing",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--t", "${env:SECRET_DISCORD_TOKEN}"
            ]
        },
        {
            "name": "Python: Production",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--t", "${env:SECRET_DISCORD_TOKEN_PROD}"
            ]
        }
    ]
}
